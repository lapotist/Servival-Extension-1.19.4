{
  "schemaVersion": 1,
  "id": "survival_extension",
  "version": "${version}",

  "name": "Survival Extension",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "lapotist"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC BY-NC-SA 4.0",
  "icon": "assets/survivalextension/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.lapotist.survivalextension.SurvivalExtension"
    ],
    "client":[
      "net.lapotist.survivalextension.SurvivalExtensionClient"
    ]
  },
  "mixins": [
    "survivalextension.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
